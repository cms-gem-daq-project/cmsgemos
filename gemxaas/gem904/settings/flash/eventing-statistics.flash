	<xmas:item name="lostdatacounthisto" infospace="urn:eventing-statistics" type="vector unsigned int 32" />
	<xmas:item name="clients" infospace="urn:eventing-statistics" type="unsigned int 32" />
	<xmas:item name="cpuusage" infospace="urn:eventing-statistics" type="double" />
	<xmas:item name="rsize" infospace="urn:eventing-statistics" type="unsigned int 32" />
	<xmas:item name="vsize" infospace="urn:eventing-statistics" type="unsigned int 32"/>
