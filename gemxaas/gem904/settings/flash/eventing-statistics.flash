	<xmas:item name="systemtime" infospace="urn:eventing-statistics" type="double" />
	<xmas:item name="messagecount" infospace="urn:eventing-statistics" type="unsigned int 32" />
	<xmas:item name="datacount" infospace="urn:eventing-statistics" type="unsigned int 32" />
